    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-split-by-path": "^2.0.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest": {
    "setupFiles": [
      "./test/jest/jestsetup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "globals": {
      "TS_CONFIG": "tsconfig.json"
    },
    "mapCoverage": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "scripts": {
    ".prepush": "npm test && test:coverage",
    ".precommit": "npm test && test:coverage",
    "postinstall": "node test/nightwatch/nightwatch.conf.js",
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --progress --colors",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-scss",
    "lint-scss": "stylelint \"./src/**/*.scss\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "start": "webpack-dev-server --hot",
    "start:server": "node dist/server/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db": "nodemon ./db",
    "dev": "NODE_ENV=development node webpack.server.js --progress --colors",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e-test": "cd test/nightwatch && node nightwatch",
    "e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "e2e-ie": "node_modules/.bin/nightwatch -e ie11"
  }
}
